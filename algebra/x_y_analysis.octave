% Find values of y when y = (x^3) + 1 and x is -10 to 10 with interval of 1
% Define the range of x values
x_values = -10:1:10; % Generates a list from -10 to 10 with an interval of 1

% Compute the corresponding y values
y_values = x_values.^3 + 1;

% Display the corresponding x and y values
disp(' x      y');
disp('-----------');
for i = 1:length(x_values)
    fprintf('%3d    %6d\n', x_values(i), y_values(i));
end
