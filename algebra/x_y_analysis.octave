% Find the roots of (x^2) -2x -3 = 0
% Define the coefficients of the quadratic equation
a = 1;
b = -2;
c = -3;

% Use the roots function to find the roots of the quadratic equation
roots_of_equation = roots([a, b, c]);

% Display the roots
disp('Roots of the quadratic equation:');
disp(roots_of_equation);
